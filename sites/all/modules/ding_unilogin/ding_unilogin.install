<?php

/**
 * @file
 * Install, update and uninstall functions for the ding_unilogin module.
 */

/**
 * Implements hook_install().
 */
function ding_unilogin_install() {
  db_update('system')
    ->fields(array(
      'weight' => 100,
    ))
    ->condition('name', 'ding_unilogin')
    ->execute();
}
