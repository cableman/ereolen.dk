+  $collection = $vars['collection'];
+  $vars['path'] = '';
+  if (!isset($collection->placeholder)) {
+    $uri = entity_uri('ting_collection', $collection);
+    $vars['path'] = url($uri['path'], $uri['options']);
+  }
+
+  if (isset($collection->placeholder)) {
+    $vars['classes_array'][] = 'placeholder';
